#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_latest-version@1.0.1@latest-version/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_latest-version@1.0.1@latest-version/cli.js" "$@"
  ret=$?
fi
exit $ret
